@GLOSSDEF{FPGA, 
	word = "FPGA", 
	definition ="Field Programmable Gate Array, dispositivo semiconductor que contiene bloques de logica cuya interconexion y funcionalidad puede ser configurada 'in situ' mediante un lenguaje de descripcion especializado."
}

@GLOSSDEF{ASIC, 
	word = "ASIC", 
	definition ="Un Circuito Integrado para Aplicaciones Especificas (o ASIC, por sus siglas en ingles application specific integrated circuit) es un circuito integrado hecho a la medida para un uso en particular, en vez de ser concebido para propositos de uso general."
}

@GLOSSDEF{TCAM, 
	word = "TCAM", 
	definition ="Variante de un circtuito de tipo CAM (Content Addressable Memory), es un tipo especifico de circuito de memoria empleada en determinadas aplicaciones que requieren velocidades de busqueda muy elevadas."
}

@GLOSSDEF{Workshop, 
	word = "Workshop", 
	definition ="Talleres de trabajo intensivo (una semana 8 10 horas diarias de trabajo); se desarrollan en grupos interdisiplinarios conformados
	habitualmente por integrantes con visiones diferentes de una misma tematica. Los workshop son dirigidos generalmente por un initado internacional que sustenta el 
	trabajo de direccion con conferencias."
}

