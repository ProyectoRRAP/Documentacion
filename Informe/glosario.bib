@GLOSSDEF{AS,word="AS", definition="Autonomous System, Conjunto de redes bajo una administracion comun que comparten una misma estrategia de ruteo."} 

@GLOSSDEF{ASIC,word="ASIC", definition="Application-Specific Integrated Circuit. Chip disen~ado  para una aplicacion especifica."}
 
@GLOSSDEF{BGP,word="BGP", definition="Protocolo de ruteo intradominio, permite intercambiar informacion de alcanzabilidad con otros sistemas BGP."}

@GLOSSDEF{BIRD,word="BIRD", definition="Suite de ruteo IP Open Source."}

@GLOSSDEF{Calidad de servicio,word="Calidad de servicio", definition="Calidad de servicio Rendimiento promedio de una red."}
 
@GLOSSDEF{CPE,word="CPE", definition="Customer provisioned Equipment Equipo(hardware) de un proveedor de servicios desplegado en las instalaciones de un cliente y conectado a la red del proveedor."}

 
@GLOSSDEF{CPLD,word="CPLD", definition="Complex Programmable Logic Device, dispositivo de hardware reprogramable."}
 
@GLOSSDEF{DoS,word="DoS", definition="Ataque a un sistema o red que causa que un servicio o recurso sea inaccesible a usuarios leg√≠timos."}
 
@GLOSSDEF{Ethertype,word="Ethertype", definition="Campo de 16 bits en el cabezal Ethernet que indica el protocolo que se encuentra en la capa superior."}
 
@GLOSSDEF{IETF,word="IETF", definition="Comunidad formada por grupos de trabajo de areas especificas responsables de desarrollar estandares de internet."}
 
@GLOSSDEF{IRTF,word="IRTF", definition="Internet Research Task Force. Comunidad de expertos en redes donde se tratan temas relacionados con internet."}
@GLOSSDEF{IS-IS,word="IS-IS", definition="Intermediate System to Intermediate System, protocolo de ruteo dinamico IP."}
 
@GLOSSDEF{LDP,word="LDP", definition="Label Distribution Protocol, Protocolo utilizado por MPLS para distrubir etiquetas entre los nodos de una red."}
 
@GLOSSDEF{LSDB,word="LSDB", definition="Link State Database, creada y utilizada por el protocolo OSPF, es la base de datos donde se almacena la topologia de la red."}
 
@GLOSSDEF{MPLS,word="MPLS", definition=" Multi Protocol Label Switching, protocolo de red basado en etiquetas que opera entre la capa de enlace y la capa de red."}
 

@GLOSSDEF{NPU,word="NPU", definition="Network Processing Unit, unidad de procesamiento orientada a aplicaciones de red."}

@GLOSSDEF{VHDL,word="VHDL", definition="Lenguaje de descripcion de hardware."}

@GLOSSDEF{Verilog,word="Verilog", definition="Lenguaje de descripcion de hardware."}

@GLOSSDEF{TCAM,word="TCAM", definition="Ternary content-addressable memory, memoria de alta velocidad que busca y accede a sus contenidos en un ciclo de reloj."}

@GLOSSDEF{Switch,word="Switch", definition="Dispositivo que reenvia paquetes en base a la direccion de capa 2 de dichos paquetes."}

@GLOSSDEF{Sistema Autonomo,word="Sistema Autonomo", definition="Ver AS."}

@GLOSSDEF{RYU,word="RYU", definition="Controlador SDN orientado a trabajar con el protocolo OpenFlow."}

@GLOSSDEF{Router,word="Router", definition="Dispositivo que reenvia paquetes en base a su direccion de capa 3."}

@GLOSSDEF{RIP,word="RIP", definition="Routing Information Protocol. Protocolo de ruteo IP basado en algoritmo vector de distancias."}

@GLOSSDEF{RFC,word="RFC", definition="Request For Comment. Documento que especifica un estandar concebido por la IETF."}

@GLOSSDEF{TAP,word="TAP", definition="Dispositivo que para interceptar el trafico en una red, para por ejemplo, su posterior analisis."}

@GLOSSDEF{RauFlow ,word="RauFlow ", definition="Aplicacion de control y gestion desarrollada en el marco de este proyecto."}

@GLOSSDEF{Rau-Switch,word="Rau-Switch", definition="Denominacion de los nodos fabricados y utilizados en el marco de este proyecto."}

@GLOSSDEF{Quagga,word="Quagga", definition=" Suite de ruteo IP Open Source."}

@GLOSSDEF{QoS,word="QOS", definition="Ver calidad de servicio."}

@GLOSSDEF{OSPF,word="OSPF", definition="Open Shortest Path First. Protocolo de ruteo IP dinamico basado basado en algoritmos de vector de distancias."}

@GLOSSDEF{Proxy,word="Proxy", definition="Intermediario entre las peticiones de red que realiza un cliente a un servidor."}

@GLOSSDEF{OpenVSwitch,word="OpenVSwitch", definition="Switch de red implementado en software."}

@GLOSSDEF{OpenFlow,word="OpenFlow", definition="Protocolo de red perteneciente a la capa de aplicacion que permite la comunicacion entre un controlador con dispositivos habilitados."}
